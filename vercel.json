{
  "version": 2,
  "installCommand": "npm install",
  "builds": [
    {
      "src": "Frontend/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/output.css",
      "dest": "/Frontend/public/output.css"
    },
    {
      "src": "/star.css",
      "dest": "/Frontend/public/star.css"
    },
    {
      "src": "/images/(.*)",
      "dest": "/Frontend/public/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/Frontend/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
